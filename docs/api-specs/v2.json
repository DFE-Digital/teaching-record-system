{
  "openapi": "3.0.1",
  "info": {
    "title": "DQT API",
    "version": "v2"
  },
  "paths": {
    "/v2/teachers/{trn}/itt-outcome": {
      "put": {
        "tags": [
          "IttOutcome"
        ],
        "summary": "Sets ITT outcome for a teacher",
        "parameters": [
          {
            "name": "trn",
            "in": "path",
            "description": "The TRN of the teacher to set ITT outcome for.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "birthdate",
            "in": "query",
            "description": "DoB of teacher",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SetIttOutcomeRequest"
              },
              "example": {"ittProviderUkprn":"1001234","outcome":"Pass","assessmentDate":"2021-12-22"}
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/SetIttOutcomeRequest"
              },
              "example": {"ittProviderUkprn":"1001234","outcome":"Pass","assessmentDate":"2021-12-22"}
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/SetIttOutcomeRequest"
              },
              "example": {"ittProviderUkprn":"1001234","outcome":"Pass","assessmentDate":"2021-12-22"}
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SetIttOutcomeResponse"
                },
                "example": {"trn":"1234567","qtsDate":"2021-12-23"}
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              },
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/v2/itt-providers": {
      "get": {
        "tags": [
          "IttProviders"
        ],
        "summary": "Gets a list of all ITT Providers",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetIttProvidersResponse"
                }
              }
            }
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/v2/teachers/find": {
      "get": {
        "tags": [
          "Teachers"
        ],
        "summary": "Returns teachers matching the specified criteria",
        "parameters": [
          {
            "name": "firstName",
            "in": "query",
            "description": "First name of person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "lastName",
            "in": "query",
            "description": "Last name of person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "previousFirstName",
            "in": "query",
            "description": "Previous first name of person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "previousLastName",
            "in": "query",
            "description": "Previous last name of person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "dateOfBirth",
            "in": "query",
            "description": "Date of birth of person",
            "schema": {
              "type": "string",
              "format": "date"
            }
          },
          {
            "name": "nationalInsuranceNumber",
            "in": "query",
            "description": "National insurance number of person",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ittProviderName",
            "in": "query",
            "description": "Name of teacher training provider",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ittProviderUkprn",
            "in": "query",
            "description": "UKPRN of teacher training provider",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "email",
            "in": "query",
            "description": "Email of person",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FindTeachersResponse"
                }
              }
            }
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/v2/teachers/update/{trn}": {
      "patch": {
        "tags": [
          "Teachers"
        ],
        "summary": "Updates a Teacher record",
        "parameters": [
          {
            "name": "trn",
            "in": "path",
            "description": "The TRN of the teacher to update",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "birthdate",
            "in": "query",
            "description": "DoB of teacher",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateTeacherRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateTeacherRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateTeacherRequest"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "No Content"
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/v2/trn-requests/{requestId}": {
      "get": {
        "tags": [
          "TrnRequests"
        ],
        "summary": "Retrieves a TRN request",
        "parameters": [
          {
            "name": "requestId",
            "in": "path",
            "description": "The unique ID the TRN request was created with.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TrnRequestInfo"
                },
                "example": {"requestId":"72888c5d-db14-4222-829b-7db9c2ec0dc3","status":"Completed","trn":"1234567"}
              }
            }
          },
          "404": {
            "description": "Not Found"
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "TrnRequests"
        ],
        "summary": "Creates a request for a TRN",
        "parameters": [
          {
            "name": "requestId",
            "in": "path",
            "description": "A unique ID that represents this request. If a request has already been created with this ID then that existing record's result is returned.",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GetOrCreateTrnRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/GetOrCreateTrnRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/GetOrCreateTrnRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TrnRequestInfo"
                },
                "example": {"requestId":"72888c5d-db14-4222-829b-7db9c2ec0dc3","status":"Completed","trn":"1234567"}
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TrnRequestInfo"
                },
                "example": {"requestId":"72888c5d-db14-4222-829b-7db9c2ec0dc3","status":"Completed","trn":"1234567"}
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
    "/v2/unlock-teacher/{teacherId}": {
      "put": {
        "tags": [
          "UnlockTeacher"
        ],
        "description": "Unlocks the teacher record allowing the teacher to sign in to the portals",
        "parameters": [
          {
            "name": "teacherId",
            "in": "path",
            "description": "The ID of the teacher record to unlock",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          },
          "404": {
            "description": "Not Found"
          },
          "429": {
            "description": "Too Many Requests",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    }
  },
